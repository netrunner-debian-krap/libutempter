#!/bin/sh
# postinst script for libutempter

set -e

case "$1" in
    configure)
        addgroup --quiet --system utempter
        chown root:utempter /usr/lib/utempter
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
